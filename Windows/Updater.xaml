<Window x:Class="SS_Tool_Box.Windows.Updater"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SS_Tool_Box.Windows"
        mc:Ignorable="d"
        Title="Updater"
        Height="580"
        Width="890"
        ResizeMode="NoResize"
        WindowStyle="None"
        AllowsTransparency="True"
        Background="Transparent"
        ShowInTaskbar="False">
    <WindowChrome.WindowChrome>
        <WindowChrome GlassFrameThickness="1,0,0,0" CornerRadius="7" ResizeBorderThickness="0,0,0,0" CaptionHeight="0" NonClientFrameEdges="None"/>
    </WindowChrome.WindowChrome>
    <Grid>
        <Border Background="{DynamicResource colorCard1}" CornerRadius="5" HorizontalAlignment="Center" VerticalAlignment="Center" Height="185" Margin="0" Width="515">

        </Border>
    </Grid>
</Window>
